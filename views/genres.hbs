<div class="container-fluid">
  <h1>Genres</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Genre</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{id}}</th>
        <td>{{genre}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>